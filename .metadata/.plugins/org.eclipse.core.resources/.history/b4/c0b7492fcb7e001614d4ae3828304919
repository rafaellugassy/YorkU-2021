package labA;

public class LabA7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = Integer.parseInt(args[0]);
		
		int mask1 = 1024; 
		int y1 = x & mask1; 
		y1 = y1 >> 10; 
		
		int mask2 = (int) Math.pow(2, 19); 
		int y2 = x & mask2; 
		y2 = y2 >> 20; 
		
		System.out.print(Integer.toBinaryString(x));

	}
}
